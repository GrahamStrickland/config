{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#C3E88D",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": "{{ .CurrentDate | date .Format }}",
          "type": "time"
        },
        {
          "foreground": "#82AAFF",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#FF5370",
          "properties": {
            "branch_icon": "",
            "fetch_status": false
          },
          "style": "plain",
          "template": " <#5FAAE8></>{{ .HEAD }}<#5FAAE8></>",
          "type": "git"
        },
        {
          "foreground": "#EEFFFF",
          "style": "plain",
          "template": " $ ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}
